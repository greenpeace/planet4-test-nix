{
	"name": "greenpeace/planet4-test-nix",
	"description": "Greenpeace P4 test-nix",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "7"
	},
	"data_from": "international",
	"scripts": {
		"core:language": "wp language core install da_DK; wp language core activate da_DK",
		"site:custom": [
			"@core:language"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/1edb2fd14ec5ec2165fa4021d1d427f474fafdb5.zip"
				}
			}
		},
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-plugin-gutenberg-blocks",
				"type": "wordpress-plugin",
				"autoload": null,
				"extra": null,
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-plugin-gutenberg-blocks/c940f5d3e079a7d708e7a508dc84e56f5e86712b.zip"
				}
			}
		}
	]
}
