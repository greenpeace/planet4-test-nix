{
	"name": "greenpeace/planet4-test-nix",
	"description": "Greenpeace P4 test-nix",
	"license": "MIT",
	"require": {
		"greenpeace/planet4-master-theme": "7",
		"greenpeace/planet4-plugin-gutenberg-blocks": "dev-master"
	},
	"data_from": "denmark",
	"scripts": {
		"core:language": "wp language core install da_DK; wp language core activate da_DK",
		"site:custom": [
			"@core:language"
		]
	},
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "greenpeace/planet4-master-theme",
				"type": "wordpress-theme",
				"autoload": {
					"psr-4": {
						"P4\\MasterTheme\\": "src/"
					}
				},
				"extra": {
					"installer-paths": {
						"vendor/plugins/{$name}/": [
							"type:wordpress-plugin"
						]
					}
				},
				"version": 7,
				"dist": {
					"type": "zip",
					"url": "https://storage.googleapis.com/planet4-packages/planet4-master-theme/8cb799d61529e97d1e447c81d127677ee49a32e2.zip"
				}
			}
		}
	]
}
